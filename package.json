    "@babel/plugin-transform-react-jsx": "7.27.1",
    "@babel/plugin-transform-typescript": "7.27.1",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@babel/traverse": "7.27.4",
    "@babel/types": "7.27.6",
    "axios": "^1.9.0",
    "chalk": "^5.3.0",
