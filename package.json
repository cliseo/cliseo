{
  "name": "cliseo",
  "version": "0.2.2",
  "description": "Instant AI-Powered SEO Optimization CLI for Developers",
  "type": "module",
  "bin": {
    "cliseo": "./dist/cli/cli.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:cli": "tsc -b tsconfig.cli.json",
    "prepare": "npm run build:cli",
